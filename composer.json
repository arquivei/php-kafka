{
  "name": "arquivei/php-kafka",
  "description": "PHP Kafka Client ",
  "keywords": [
    "php",
    "kafka",
    "client"
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^7.4",
    "ext-rdkafka": "3.1.*",
    "psr/log": "^1.1",
    "monolog/monolog": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-phpunit": "^0.12",
    "squizlabs/php_codesniffer": "3.5.*"
  },
  "suggest": {
    "monolog/monolog": "2.*"
  },
  "autoload": {
    "psr-4": {
      "PHP\\Kafka\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
